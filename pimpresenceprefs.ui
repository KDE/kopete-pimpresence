<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>PIMPresencePrefsUI</class>
<widget class="QWidget">
    <property name="name">
        <cstring>PIMPresencePrefsUI</cstring>
    </property>
    <property name="enabled">
        <bool>true</bool>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>426</width>
            <height>165</height>
        </rect>
    </property>
    <property name="caption">
        <string></string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>11</number>
        </property>
        <property name="spacing">
            <number>6</number>
        </property>
        <widget class="QButtonGroup">
            <property name="name">
                <cstring>buttonGroup4</cstring>
            </property>
            <property name="title">
                <string>Presence options</string>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>rbPresenceOnly</cstring>
                    </property>
                    <property name="text">
                        <string>Set presence onl&amp;y</string>
                    </property>
                    <property name="accel">
                        <string>Alt+Y</string>
                    </property>
                </widget>
                <widget class="QRadioButton">
                    <property name="name">
                        <cstring>rbPresenceDescription</cstring>
                    </property>
                    <property name="text">
                        <string>Set presence &amp;and description</string>
                    </property>
                    <property name="accel">
                        <string>Alt+A</string>
                    </property>
                    <property name="checked">
                        <bool>true</bool>
                    </property>
                </widget>
                <widget class="QCheckBox">
                    <property name="name">
                        <cstring>cbPrivateDescriptions</cstring>
                    </property>
                    <property name="text">
                        <string>Show description for events marked as Private</string>
                    </property>
                    <property name="accel">
                        <string></string>
                    </property>
                </widget>
            </vbox>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>rbPresenceDescription</sender>
        <signal>toggled(bool)</signal>
        <receiver>cbPrivateDescriptions</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>rbPresenceOnly</tabstop>
    <tabstop>rbPresenceDescription</tabstop>
    <tabstop>cbPrivateDescriptions</tabstop>
</tabstops>
<layoutdefaults spacing="6" margin="11"/>
</UI>
